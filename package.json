{
  "name": "xonos.js",
  "version": "1.0.0",
  "description": "JavaScript wrapper for XonosTools made in TypeScript",
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:watch": "rimraf dist && tsc-watch --noClear --onSuccess \"node dist/index.js\"",
    "start": "node dist/index.js",
    "start:watch": "nodemon dist/index.js",
    "prettier": "prettier --write src",
    "prettier:check": "prettier --check src"
  },
  "main": "dist",
  "repository": "https://github.com/marinofranz/xonos.js.git",
  "author": "Marino Franz <mvbjarnasynir@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/node": "^16.11.11",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^0.24.0"
  }
}
